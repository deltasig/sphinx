@model DeltaSigmaPhiWebsite.Entities.WorkOrder
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Work Order";
    ViewBag.Subtitle = Model.WorkOrderId;
}

@section Title
{
    @Html.Partial("_Title")
}

<div class="container">
    <div class="row">
        <div class="col-xs-6">
            <p>
                <a href="@Url.Action("Index")" class="btn btn-sm btn-default">
                    <i class="fa fa-caret-left"></i> Back to Work Orders
                </a>
            </p>
        </div>
        <div class="col-sm-6 text-right">
            <p>
                @if (User.IsInRole("Administrator") || User.IsInRole("House Manager") ||
                     Model.UserId == WebSecurity.CurrentUserId)
                {
                    <a href="@Url.Action("Edit", new { id = Model.WorkOrderId })" class="btn btn-sm btn-info">
                        <i class="fa fa-wrench"></i> Edit
                    </a>
                }
            </p>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script>

    </script>
}
