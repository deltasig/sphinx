@model DeltaSigmaPhiWebsite.Entities.StudyPeriod
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Delete Assignment";
}
@section Title
{
    @Html.Partial("_Title")
}
<div class="container">
    <div class="row">
        <div class="col-sm-6">
            <p>
                <a href="@Url.Action("Index")" class="btn btn-sm btn-default">
                    <i class="fa fa-caret-left"></i> Back to Study Periods
                </a>
            </p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-6">
            <div class="alert alert-danger">
                <p>Are you sure you want to delete this <strong>Assignment</strong>?</p>
            </div>

            <div class="form-group">
                <dl class="dl-horizontal">
                    <dt>Start Date/Time</dt>
                    <dd>@Model.Start</dd>

                    <dt>End Date/Time</dt>
                    <dd>@Model.End</dd>
                </dl>
            </div>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-group">
                    <input type="submit" value="Delete" class="btn btn-default" /> 
                </div>
            }
        </div>
    </div>
</div>
