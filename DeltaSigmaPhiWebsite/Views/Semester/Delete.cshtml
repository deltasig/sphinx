@model Semester

@{
    ViewBag.Title = "Delete Semester";
}
@section Title
{
    @Html.Partial("_Title")
}
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h3>Are you sure you want to delete this?</h3>

            <blockquote>
                <h4>@Model.ToString()</h4>
                <h5>
                    @Html.DisplayFor(model => model.DateStart) to
                    @Html.DisplayFor(model => model.DateEnd)
                </h5>
            </blockquote>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-group">
                    <input type="submit" value="Delete" class="btn btn-default" />
                </div>
            }
            <div>
                @Html.ActionLink("Back to List", "Index")
            </div>
        </div>
    </div>
</div>
