@model DeltaSigmaPhiWebsite.Models.AdminPanelModel

@{
    ViewBag.Title = "Admin Panel";
}

<div class="container">
    <div class="page-header">
        <div class="row">
            <div class="col-sm-6">
                <h1>@ViewBag.Title </h1>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <p class="text-success">@ViewBag.SuccessMessage</p>
    <p class="text-warning">@ViewBag.FailMessage</p>

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="row">
        <div class="col-sm-3">
            @{ Html.RenderPartial("_AddSemesterPartial", Model.SemesterModel); }
        </div>
        <div class="col-sm-3">
            
        </div>
        <div class="col-sm-3">

        </div>
        <div class="col-sm-3">
            
        </div>
    </div>

</div>
