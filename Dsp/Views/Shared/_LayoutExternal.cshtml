<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @RenderSection("AdditionalMeta", false)
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/Images/Favicon-16x16.png" />
    <link rel="stylesheet" type="text/css" href="~/Content/OpenSans.css">
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">
    @Styles.Render("~/Content/custom-css")
    @Styles.Render("~/Content/font-awesome")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
</head>
<body style="padding: 0" class="external-body">
    @Html.Partial("_NavExternal")
    @RenderSection("Title", required: false)
    @RenderBody()
    @Html.Partial("Footer")

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
